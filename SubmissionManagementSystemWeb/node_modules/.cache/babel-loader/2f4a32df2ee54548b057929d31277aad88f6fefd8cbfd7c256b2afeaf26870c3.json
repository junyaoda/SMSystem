               </template>\n                </v-data-table>\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n    beforeRouteUpdate(to, from, next) {\n        this.message = to.params.message;\n        confirm(this.message);\n        this.selectAll()\n        next()\n    },\n    data() {\n        return {\n            message: this.$route.params.message,\n            result: false,\n            users: [],\n            headers: [\n                {\n                    text: 'ユーザ名',\n                    value: 'userName',\n                },\n                {\n                    text: 'SlackID',\n                    value: 'slackId',\n                },\n                {\n                    text: '操作',\n                    value: 'delete',\n                    align: 'center',\n                    sortable: false,\n                },\n            ],\n        }\n    },\n    created() {\n        this.selectAll()\n        if (this.message != null) {\n            this.result = true;\n            setTimeout(() => { this.result = false; }, 3000);\n        }\n    },\n    methods: {\n        selectAll() {\n            const path = 'http://localhost:8080/user/selectUserAll';\n            axios.get(path)\n                .then(response => {\n                    this.users = response.data\n                })\n                .catch(error => {\n                    console.log(error)\n                })\n        },\n        insert() {\n            this.$router.push(\n                \"MemberInsert\"\n            );\n        },\n        del(id) {\n            var res = confirm(\"削除してもよろしいですか？\");\n            if (res == true) {\n                const path = 'http://localhost:8080/user/deleteUser?id=' + id\n                axios.delete(path\n                )\n                    .then(response => {\n                        console.log(response.data);\n\n                        this.$router.push({\n                            name: \"MemberManagement\",\n                            params: {\n                                message: \"DeleteComplete!!\"\n                            }\n                        });\n\n                    })\n                    .catch(err => {\n                        console.log(\"err:\", err);\n                    });\n            }\n        },\n        update(id, userName, slackId) {\n            this.$router.push({\n                name: \"MemberUpdate\",\n                params: {\n                    id: id,\n                    userName: userName,\n                    slackId: slackId,\n                }\n            });\n        },\n    }\n}\n</script>\n\n<style scoped>\n.fade-enter-active,\n.fade-leave-active {\n    transition: opacity .5s;\n}\n\n.fade-enter,\n.fade-leave-to {\n    opacity: 0;\n}\n</style>"],"mappings":";AAgDA,OAAAA,KAAA;AACA;EACAC,kBAAAC,EAAA,EAAAC,IAAA,EAAAC,IAAA;IACA,KAAAC,OAAA,GAAAH,EAAA,CAAAI,MAAA,CAAAD,OAAA;IACAE,OAAA,MAAAF,OAAA;IACA,KAAAG,SAAA;IACAJ,IAAA;EACA;EACAK,KAAA;IACA;MACAJ,OAAA,OAAAK,MAAA,CAAAJ,MAAA,CAAAD,OAAA;MACAM,MAAA;MACAC,KAAA;MACAC,OAAA,GACA;QACAC,IAAA;QACAC,KAAA;MACA,GACA;QACAD,IAAA;QACAC,KAAA;MACA,GACA;QACAD,IAAA;QACAC,KAAA;QACAC,KAAA;QACAC,QAAA;MACA;IAEA;EACA;EACAC,QAAA;IACA,KAAAV,SAAA;IACA,SAAAH,OAAA;MACA,KAAAM,MAAA;MACAQ,UAAA;QAAA,KAAAR,MAAA;MAAA;IACA;EACA;EACAS,OAAA;IACAZ,UAAA;MACA,MAAAa,IAAA;MACArB,KAAA,CAAAsB,GAAA,CAAAD,IAAA,EACAE,IAAA,CAAAC,QAAA;QACA,KAAAZ,KAAA,GAAAY,QAAA,CAAAf,IAAA;MACA,GACAgB,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;MACA;IACA;IACAG,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA,CACA,cACA;IACA;IACAC,IAAAC,EAAA;MACA,IAAAC,GAAA,GAAA3B,OAAA;MACA,IAAA2B,GAAA;QACA,MAAAb,IAAA,iDAAAY,EAAA;QACAjC,KAAA,CAAAmC,MAAA,CAAAd,IACA,EACAE,IAAA,CAAAC,QAAA;UACAG,OAAA,CAAAC,GAAA,CAAAJ,QAAA,CAAAf,IAAA;UAEA,KAAAqB,OAAA,CAAAC,IAAA;YACAK,IAAA;YACA9B,MAAA;cACAD,OAAA;YACA;UACA;QAEA,GACAoB,KAAA,CAAAY,GAAA;UACAV,OAAA,CAAAC,GAAA,SAAAS,GAAA;QACA;MACA;IACA;IACAC,OAAAL,EAAA,EAAAM,QAAA,EAAAC,OAA